    <section class="contact section" id="enter">
      <div class="container">
        <div class="col">
          <div class="">
            <h2 class=" " data-aos="fade-up" data-aos-delay="200">
              Upload your solution document
            </h2>

            <form
              action="/"
              method="post"
              class="contact-form webform"
              data-aos="fade-up"
              data-aos-delay="400"
              role="form"
            >
              <input
                type="text"
                class="form-control"
                name="name"
                placeholder="Full Name"
              />
              <input
                type="email"
                class="form-control"
                name="email"
                placeholder="Email"
              />
              <input
                type="number"
                class="form-control"
                name="phone"
                placeholder="Phone Number"
              />

              <input
                type="text"
                class="form-control"
                name="college"
                placeholder="Enter the full name of your College"
              />

              <input
                type="text"
                class="form-control"
                name="branch"
                placeholder="Branch"
              />
              <input
                type="text"
                class="form-control"
                name="question"
                placeholder="Question Number"
              />

              <label for="troubleshoot">
                <h6>What is wrong with our solution?</h6>
              </label>
              <textarea
                class="form-control"
                id="troubleshoot"
                rows="5"
                name="solution"
                placeholder="Add your solution here"
              ></textarea>
              <button type="submit" class="btn-success">Submit</button>
            </form>
          </div>
        </div>
      </div>
    </section>

